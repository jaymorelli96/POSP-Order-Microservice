pipeline { 
    agent any 

    stages { 
        stage('Build') {
            steps {
                echo 'Clean Build'
                bat 'mvn clean compile'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
                bat 'mvn test'
            }
        }
		stage('jacoco') {
			steps {
			echo 'Jacoco Coverage'
			jacoco()
			}
		}		
		
	}


	post {
		always {
			cleanWs()
		}
	}

}
